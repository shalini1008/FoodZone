{
  "version": 2,
  "builds": [
    {
      "src": "src/**/index.js",
      "use": "@vercel/static-build"
    },
    {
      "src": "server/food.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/food",
      "dest": "/server/food.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/index.html"
    }
  ]
}

