{
  "routes": [
    {
      "src": "/api/food",
      "dest": "/server/food.js"  // API route to /server/food.js
    },
    {
      "src": "/images/(.*)",  // Route for image assets
      "dest": "/server/public/images/$1"  // Static images are in /server/public/images
    },
    {
      "src": "/(.*)",  // Route for frontend files
      "dest": "/dist/index.html"  // Ensure this points to your frontend (use dist or another appropriate path)
    }
  ]
}
